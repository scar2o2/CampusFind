create table public.notifications2 (
  id bigint generated by default as identity not null,
  lost_user_id bigint not null,
  found_user_id bigint not null,
  lostitemid bigint not null,
  founditemid bigint not null,
  message text not null,
  isread boolean null default false,
  created_at timestamp with time zone null default now(),
  lost_user_phone text null,
  found_user_phone text null,
  type text not null,
  constraint notifications2_pkey primary key (id)
) TABLESPACE pg_default;